{
  "name": "fontmon",
  "version": "1.0.0",
  "description": "A fontmanger made with electron",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild.cmd",
    "build": "npm run rebuild && next build renderer && next export renderer",
    "dist": "npm run build && build --dir"
  },
  "build": {
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ]
  },
  "keywords": [],
  "author": "Andy Bitz",
  "license": "MIT",
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "inline-react-svg"
    ]
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "electron-builder-squirrel-windows": "^19.46.0",
    "electron-is-dev": "^0.3.0",
    "electron-next": "^3.1.3"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^0.4.0",
    "electron": "^1.8.2-beta.2",
    "electron-builder": "^19.45.5",
    "electron-rebuild": "^1.6.0",
    "next": "^4.1.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
